{
    "name": "bus-tracking-system",
    "version": "1.0.0",
    "description": "Real-time bus tracking and route management system",
    "main": "src/index.js",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "dev": "sls offline start",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "lint": "eslint src/ tests/",
        "lint:fix": "eslint src/ tests/ --fix",
        "format": "prettier --write src/ tests/",
        "deploy:dev": "sls deploy --stage dev",
        "deploy:prod": "sls deploy --stage prod",
        "seed:data": "node scripts/seed-data.js",
        "dynamodb:install": "sls dynamodb install",
        "dynamodb:start": "sls dynamodb start"
    },
    "dependencies": {
        "aws-sdk": "^2.1490.0",
        "ioredis": "^5.3.2",
        "joi": "^17.11.0",
        "uuid": "^9.0.1",
        "ngeohash": "^0.6.3",
        "http-status-codes": "^2.3.0"
    },
    "devDependencies": {
        "serverless": "^3.35.0",
        "serverless-offline": "^13.2.0",
        "serverless-dynamodb-local": "^0.2.40",
        "jest": "^29.7.0",
        "eslint": "^8.52.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.29.0",
        "prettier": "^3.0.3",
        "supertest": "^6.3.3",
        "aws-sdk-mock": "^5.8.0"
    },
    "keywords": [
        "serverless",
        "aws",
        "lambda",
        "dynamodb",
        "redis",
        "bus-tracking",
        "real-time"
    ],
    "author": "AKASH",
    "license": "MIT"
}